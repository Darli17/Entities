while true do
    wait(0.)
	local MonsterMath = math.random(1, 1500)
	if MonsterMath == 52 then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Darli17/Entities/refs/heads/main/Entities/Ripper"))()
	end
	if MonsterMath == 45 then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Darli17/Entities/refs/heads/main/Entities/Cease"))()
	end
	if MonsterMath == 55 then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Darli17/Entities/refs/heads/main/Entities/Frostbite"))()
	end
	if MonsterMath == 100 then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Darli17/Entities/refs/heads/main/Entities/DearGod"))()
	end
	if MonsterMath == 65 then
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Darli17/Entities/refs/heads/main/Entities/Rebound"))()
	end
end
